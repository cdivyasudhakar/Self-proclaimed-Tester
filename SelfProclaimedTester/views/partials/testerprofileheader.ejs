<html>
  <head>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.0-beta.2/angular.js" type="text/javascript"></script>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load('visualization', '1', {packages:['corechart',"corechart"]});
      google.setOnLoadCallback(function() {
  angular.bootstrap(document.body, ['myApp.controllers']);
});
    </script>
	<script>
	angular.module('myApp.controllers', []).
  controller('MyCtrl1', ['$scope',
    function($scope) {
      var data = google.visualization.arrayToDataTable([
        ['Year', 'Sales', 'Expenses'],
        ['2004', 8000, 400],
        ['2005', 1170, 460],
        ['2006', 660, 1120],
        ['2007', 1030, 540]
      ]);
      var options = {
        title: 'At my level'
      };
      var chart = new google.visualization.LineChart(document.getElementById('chartline'));

      chart.draw(data, options);
	  
	      var chart2 = new google.visualization.PieChart(document.getElementById('chartpie'));

      chart2.draw(data, options);
    }
  ])
    .controller('MyCtrl2', [
      function() {

      }
  ]);
  </script>
  </head>
  <body>
   <!-- <div id="chart_div" style="width: 900px; height: 500px;"></div> -->
	<div ng-controller="MyCtrl1" id="chartline"  style="width: 450px; height: 450px;"></div>
	<div ng-controller="MyCtrl1" id="chartpie" style="width: 450px; height: 450px;"></div>
  </body>
</html>